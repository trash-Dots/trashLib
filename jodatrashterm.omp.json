{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{.UserName}} in ~/{{.Folder}}{{if .Root}} :: root{{end}}",
  "palette": {
    "micolor": "#758e6a",
    "red": "#e06c75",
    "green": "#98c379"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "template": "\u250c\u2500\u2500",
          "foreground": "#d2ff5e",
          "type": "text",
          "style": "plain"
        },
        {
          "background": "#424242",
          "foreground": "#d2ff5e",
          "style": "diamond",
          "template": " {{ .CurrentDate | date .Format }} ",
          "trailing_diamond": "\ue0b0",
          "shell": "bash",
          "type": "time",
          "properties": {
            "style": "full",
            "time_format": "Mon"
          }
        },
        {
          "background": "#666666",
          "foreground": "#e2e2e2",
          "style": "diamond",
          "template": "{{ .CurrentDate | date .Format }} ",
          "trailing_diamond": "\ue0b0",
          "shell": "bash",
          "type": "time",
          "properties": {
            "style": "full",
            "time_format": " 3:04"
          }
        },

        {
          "background": "#333333",
          "foreground": "#D4AAFC",
          "leading_diamond": "<transparent,background>\uE0B0</>",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "style": "folder"
          },
          "style": "diamond",
          "template": " <#DCB977>local:(</>{{ .Path }}<#DCB977>)</> ",
          "type": "path"
        },

        {
          "background": "#424242",
          "foreground": "#56B6C2",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:red{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#424242{{ end }}",
            "{{ if gt .Ahead 0 }}p:green{{ end }}",
            "{{ if gt .Behind 0 }}#e2e2e2{{ end }}"
          ],
          "leading_diamond": "<#333333>\uE0B0</><#62ED8B> ➜</>",
          "trailing_diamond": "\ue0b4",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "branch_icon": ""
          },
          "style": "diamond",
          "template": " <#DCB977>git:(</>{{ .HEAD }}<#DCB977>)</> {{ if (gt .Ahead 0)}}<#56b6c2>{{ .BranchStatus }}</>{{ end }}{{ if (gt .Behind 0)}}<#56b6c2>{{ .BranchStatus }}</>{{ end }}{{ if .Staging.Changed }} <p:green>{{ .Staging.String }}</>{{ end }} <#2e2e2e>\u2502</> {{ if .Working.Changed }}<p:red>{{ .Working.String }}</p:red>{{ end }}</> ",
          "type": "git"
        }
      ]
    },

    {
      "type": "prompt",
      "alignment": "right",
      "newline": false,
      "segments": [
        {
          "background": "#666666",
          "foreground": "#e2e2e2",
          "invert_powerline": true,
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "template": " {{ .Output }} ",
          "type": "command",
          "properties": {
            "command": "git log --pretty=format:%cr -1 || date +%H:%M:%S",
            "shell": "bash"
          }
        },
        {
          "background": "#424242",
          "foreground": "#dddddd",
          "invert_powerline": true,
          "style": "diamond",
          "template": "{{ if eq \"False\" (title (default \"False\" .Env.DISABLE_SEGMENT_PRIMARY_EXEC_TIME)) }} {{ .FormattedMs }}{{ end }} ",
          "type": "executiontime",
          "properties": {
            "style": "austin",
            "threshold": 1
          }
        },

        {
          "background": "#89a3ac",
          "foreground": "#363636",
          "invert_powerline": true,
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "type": "shell",
          "template": " {{ .Name }} ",
          "trailing_diamond": ""
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#ed6262",
          "style": "diamond",
          "template": "\u2502 ✗ External command failed",
          "type": "status"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "template": "\u2514\u2500",
          "foreground": "#d2ff5e",
          "type": "text",
          "style": "plain"
        },
        {
          "template": " $",
          "foreground": "#d2ff5e",
          "type": "text",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
